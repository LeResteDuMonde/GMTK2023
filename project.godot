config_version=5

[application]

config/name="Dans ma boutique ?!"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
boot_splash/fullsize=false
config/icon="res://XTRA/icon.png"
config/windows_native_icon="res://XTRA/icon.ico"

[autoload]

InputManager="*res://script/manager/inputManager.gd"
GameManager="*res://script/manager/gameManager.gd"
AudioManager="*res://script/manager/audioManager.gd"
TimerManager="*res://script/manager/timerManager.gd"
TranslationManager="*res://script/manager/translationManager.gd"
MoneyManager="*res://script/manager/moneyManager.gd"
HealthManager="*res://script/manager/healthManager.gd"
CursorManager="*res://script/manager/cursorManager.gd"
TitleManager="*res://script/manager/titleManager.gd"
ShakeManager="*res://script/manager/shakeManager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/borderless=true
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/scale=0.67

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

